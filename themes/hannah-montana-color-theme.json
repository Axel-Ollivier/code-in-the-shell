  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#ffaacc", "fontStyle": "italic" }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": { "foreground": "#cc0066" }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape", "constant.other.placeholder"],
      "settings": { "foreground": "#ffd700" }
    },
    {
      "name": "RegEx",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#da70d6" }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#cc8800" }
    },
    {
      "name": "Language Constant",
      "scope": ["constant.language"],
      "settings": { "foreground": "#9370db" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control"],
      "settings": { "foreground": "#ff1493", "fontStyle": "bold" }
    },
    {
      "name": "Keyword Operator",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#da70d6" }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#9370db", "fontStyle": "bold italic" }
    },
    {
      "name": "Function Name",
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#aa0088" }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call entity.name.function", "support.function"],
      "settings": { "foreground": "#cc22aa" }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#dd66aa", "fontStyle": "italic" }
    },
    {
      "name": "Class / Type",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": { "foreground": "#9370db" }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#8b0051" }
    },
    {
      "name": "This / Self",
      "scope": ["variable.language.self", "variable.language.this"],
      "settings": { "foreground": "#ff1493", "fontStyle": "bold italic" }
    },
    {
      "name": "Object Key",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": { "foreground": "#aa0077" }
    },
    {
      "name": "Tag Name",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#ff1493", "fontStyle": "bold" }
    },
    {
      "name": "Tag Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#9370db" }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#ff1493", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#cc0066", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#9370db", "fontStyle": "italic" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": { "foreground": "#cc0044", "fontStyle": "strikethrough" }
    }
  ],

  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class":      "#9370db",
    "interface":  { "foreground": "#9370db", "italic": true },
    "function":   "#aa0088",
    "method":     "#cc22aa",
    "keyword":    { "foreground": "#ff1493", "bold": true },
    "modifier":   { "foreground": "#9370db", "italic": true },
    "string":     "#cc0066",
    "number":     "#cc8800",
    "operator":   "#da70d6",
    "variable":   "#8b0051",
    "parameter":  { "foreground": "#dd66aa", "italic": true },
    "property":   "#aa0077",
    "comment":    { "foreground": "#ffaacc", "italic": true }
  }
}
